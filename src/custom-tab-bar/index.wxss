.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 140rpx;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
}
.tab-bar-bg{
  position: absolute;
  width: 100%;
  height: 100%;
  background: #fff;
  top:40rpx;
}
.tab-bar-border {
  /* background-color: rgba(0, 0, 0, 0.33); */
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1rpx;
  transform: scaleY(0.5);
}

.tab-bar-item {
  position: relative;
  top:10rpx;
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.tab-bar-item navigator cover-image {
  position: relative;
  top: 10rpx;
  width: 52rpx;
  height: 52rpx;
}  
.tab-bar-item:nth-child(3) navigator cover-image{
  position: relative;
  left:3rpx;

}
.tab-bar-item:nth-child(5) navigator cover-image{
  position: relative;
  top:-20rpx;
  width: 100rpx;
  height:89rpx; 
  margin: 0rpx;
}
.tab-bar-item:nth-child(5) navigator cover-view{
  position: relative;
  top:-22rpx;
}

.tab-bar-item navigator cover-view {
  margin-top:12rpx;
  font-size: 14rpx;
}
